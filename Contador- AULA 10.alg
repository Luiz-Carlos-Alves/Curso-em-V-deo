algoritmo "CONTADOR REPITA"
// Função :Contar os números
// Autor :luiz carlos alves
// Data : 29/10/2021
// Seção de Declarações 
var

    cont,N, S : inteiro

inicio
       cont <- 1
       N <- 7
       repita
       S <- N * cont
       escreval (N , "x", cont, "=", )
       cont <- cont + 1
       
       ate (cont > 10)
// Seção de Comandos 
fimalgoritmo
